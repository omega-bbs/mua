{
  "name": "@omega-bbs/mua",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./scripts/start.js --port \"${PORT:-8080}\" --api-port=\"${API_PORT:-8000}\"",
    "build": "webpack --config ./webpack/multi.config.js",
    "clean": "rm -rf ./dist",
    "dev": "node ./scripts/dev.js --port \"${PORT:-8080}\" --api-port=\"${API_PORT:-8000}\"",
    "docker:start": "docker run --rm --publish \"${PORT:-8080}:80\" omegabbs/mua",
    "docker:build": "docker build --tag omegabbs/mua .",
    "analyze": "webpack-bundle-analyzer ./dist/client/stats.json",
    "test": "npm run lint && npm run jest",
    "jest": "jest --coverage --setupTestFrameworkScriptFile ./jest.js",
    "report-coverage": "cat ./coverage/lcov.info | codecov",
    "lint": "eslint .",
    "prettier": "eslint --fix .",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "dependencies": {
    "babel-polyfill": "^7.0.0-beta.0",
    "babel-runtime": "^7.0.0-beta.0",
    "classnames": "^2.2.5",
    "dom4": "^1.8.5",
    "draft-js": "^0.10.1",
    "draft-js-plugins-editor": "^2.0.0-rc7",
    "express": "^4.15.4",
    "mobx": "^3.2.2",
    "mobx-react": "^4.2.2",
    "pepjs": "^0.4.3",
    "polished": "^1.7.0",
    "prop-types": "^15.5.10",
    "react": "^16.0.0-rc.3",
    "react-apollo": "^1.4.15",
    "react-dom": "^16.0.0-rc.3",
    "react-helmet": "^5.2.0",
    "react-hot-loader": "^3.0.0-beta.7",
    "react-router-dom": "^4.2.2",
    "react-transition-group": "^2.2.0",
    "redraft": "^0.8.0",
    "stats-webpack-plugin": "^0.6.1",
    "styled-components": "^2.1.2",
    "yargs": "^8.0.2"
  },
  "devDependencies": {
    "babel-core": "^7.0.0-beta.0",
    "babel-eslint": "^8.0.0",
    "babel-jest": "^21.0.2",
    "babel-loader": "^7.1.2",
    "babel-minify-webpack-plugin": "^0.2.0",
    "babel-plugin-styled-components": "^1.2.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^7.0.0-beta.0",
    "babel-plugin-transform-runtime": "^7.0.0-beta.0",
    "babel-preset-env": "^2.0.0-beta.0",
    "babel-preset-react": "^7.0.0-beta.0",
    "babel-preset-stage-0": "^7.0.0-beta.0",
    "case-sensitive-paths-webpack-plugin": "^2.1.1",
    "codecov": "^2.3.0",
    "css-loader": "^0.28.7",
    "eslint": "^4.6.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-prettier": "^2.2.0",
    "eslint-plugin-react": "^7.3.0",
    "friendly-errors-webpack-plugin": "^1.6.1",
    "http-proxy-middleware": "^0.17.4",
    "husky": "^0.14.3",
    "jest": "^21.1.0",
    "jest-styled-components": "^4.6.0",
    "lint-staged": "^4.1.3",
    "opn": "^5.1.0",
    "prettier": "^1.6.1",
    "react-test-renderer": "^16.0.0-rc.3",
    "style-loader": "^0.18.2",
    "svg-react-loader": "^0.4.5",
    "svgo": "^0.7.2",
    "svgo-loader": "^1.2.1",
    "webpack": "^3.5.6",
    "webpack-bundle-analyzer": "^2.9.0",
    "webpack-dev-middleware": "^1.12.0",
    "webpack-hot-middleware": "^2.19.1",
    "webpack-hot-server-middleware": "^0.1.0",
    "webpack-node-externals": "^1.6.0"
  }
}
